---
import Layout from '../layouts/Layout.astro'
import Header from '../components/layout/Header.astro'
import Footer from '../components/layout/Footer.astro'
import VSSPage from '../components/pages/VSSPage.astro'


const seo = {
  title: 'Vendas Sem Segredos - Programa de 90 Dias | Joel Burigo',
  description:
    'Programa completo de vendas escaláveis. Aprenda e implemente o Framework dos 6Ps em 90 dias. 15 módulos, 66 aulas práticas, CRM incluído e 48 mentorias ao vivo.',
  keywords:
    'vendas sem segredos, programa vendas, curso vendas B2B, CRM vendas, framework 6Ps, mentoria vendas',
}
---

<Layout title={seo.title} description={seo.description} keywords={seo.keywords}>
  <Header />
  <VSSPage />
  <Footer />
</Layout>
